			steps{
				echo "building"
			}
		}
		stage("test"){
			steps{
				echo "testing"
			}
		}
		stage("linting"){
			steps{
				echo "linting"
			}
		}
		stage("artifcat"){
			steps{
				echo "artifacting"
			}
		}
		stage("deploy"){
			steps{
				echo "deploying now"
			}
		}
		stage("Pushing"){
			steps{
				echo "pushing now"
			}
		}
	}
}
